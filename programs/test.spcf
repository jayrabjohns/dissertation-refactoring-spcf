x = error1;
y = error2;

addLeftTerm = \f:Nat->Nat->Nat => \x:Nat => \y:Nat => if x then y else (succ (f (pred x) y));
addRightTerm = \f:Nat->Nat->Nat => \x:Nat => \y:Nat => if y then x else (succ (f x (pred y)));

add = \x:Nat => \y:Nat => (fix addLeftTerm) x y;
add' = \x:Nat => \y:Nat => (fix addRightTerm) x y;

eval (add x y);

eval (
    add' x y
);